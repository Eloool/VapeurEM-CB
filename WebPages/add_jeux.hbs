<form method="post">
    <label for="jeux">Donner moi un nom jeu:</label>
    <input type="text" id="jeux" name="jeux" required/><br>
    <label for="game">Une description:</label>
    <input type="text" id="description" name="description" required/><br>
    <label for="date">Date:</label>
    <input type="date" id="date" name="date" required/><br>
    <select name="editor" id="editor">
    <option value="">--Please choose an editor--</option>
    {{#each editor}}
    <option value="{{id}}">{{name}}</option>
    {{/each}}
    </select>
    <select name="genre" id="genre">
    <option value="">--Please choose an genre</option>
    {{#each genre}}
    <option value="{{id}}">{{name}}</option>
    {{/each}}
    </select>
    <button type="submit">Ajouter une jeux</button>
</form>
